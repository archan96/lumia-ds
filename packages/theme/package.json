{
    "name": "@lumia/theme",
    "version": "0.0.0",
    "description": "React ThemeProvider for applying Lumia token CSS variables",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run"
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "dependencies": {
        "@lumia/tokens": "workspace:*"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "happy-dom": "^20.0.10",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tsup": "^8.0.0",
        "vitest": "^2.1.9"
    }
}
