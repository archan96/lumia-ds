{
    "name": "@lumia/runtime",
    "version": "0.0.0",
    "description": "Runtime schema definitions for Lumia page rendering",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run"
    },
    "peerDependencies": {
        "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2"
  },
  "dependencies": {
    "@lumia/components": "workspace:*",
    "@lumia/forms": "workspace:*",
    "@lumia/layout": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
        "@vitest/coverage-v8": "^2.1.9",
        "happy-dom": "^20.0.10",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tsup": "^8.5.1",
        "vitest": "^2.1.9"
    }
}
